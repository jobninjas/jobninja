{
    "rewrites": [
        {
            "source": "/api/interview/:path*",
            "destination": "/api/interview/:path*"
        },
        {
            "source": "/api/:path*",
            "destination": "https://nova-ninjas-production.up.railway.app/api/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}